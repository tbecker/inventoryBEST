<h1>Listing product_dimensions</h1>

<table>
  <tr>
    <th>Company</th>
    <th>Name</th>
    <th>Type</th>
    <th>Source</th>
    <th>Product line</th>
    <th>Classification</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @product_dimensions.each do |product_dimension| %>
  <tr>
    <td><%= product_dimension.company %></td>
    <td><%= product_dimension.name %></td>
    <td><%= product_dimension.type %></td>
    <td><%= product_dimension.source %></td>
    <td><%= product_dimension.product_line %></td>
    <td><%= product_dimension.classification %></td>
    <td><%= link_to 'Show', product_dimension %></td>
    <td><%= link_to 'Edit', edit_product_dimension_path(product_dimension) %></td>
    <td><%= link_to 'Destroy', product_dimension, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Product dimension', new_product_dimension_path %>
